<%= form_for @event do |f| %>
  <%= f.label :title %><br />
  <%= f.text_field :title %><br />
  <%= f.label :body %><br />
  <%= f.text_area :body %><br />
  <%= f.label :place %><br />
  <%= f.text_field :place %><br />
  <%= f.label :date %><br />
  <%= f.date_field :date %><br />
  <%= f.submit "Create Event" %>
<% end %>
