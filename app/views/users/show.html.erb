<h2>  ID: <%=@user.id %>  </h2>
<h2>  Username: <%=@user.username %></h2>
<h2>  Email: <%=@user.email %>  </h2>
<%= link_to  "Delete Account", user_path, method: :delete %>

<h1>  User Events: </h1>

<ul>
  <% @events.each do |event| %>
    <li>
    <%= event.date %>
    <%= event.title %>
    <%= event.body %>
    <%= event.place %>
    <%= link_to 'show', event_path(event) %>
    </li>
  <% end %>
</ul>
